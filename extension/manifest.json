{
  "manifest_version": 2,
  "name": "MoodsWeb",
  "version": "1.0.0",
  "description": "A minimalist journaling interface that replaces the new tab page with daily reflection questions.",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "storage"
  ],

  "icons": {
    "16": "icons/icon.svg",
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg"
  },

  "author": "MoodsWeb Team",
  "homepage_url": "https://github.com/Aarav2709/MoodsWeb"
}
